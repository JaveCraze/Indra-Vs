script/source = "extends CharacterBody2D
@onready var anim = $CharacterAnimator
var basespeed = 430.0
var slidespeed = 600
var walljumpbegin = false
var SPEED = basespeed
var accel = .09
var jumpresistance = .74
const JUMP_VELOCITY = -800.0
var damage = 1
var walljumpvelocity = 570
var airattackvelocity = 500
var attackvelocity = 700
var attackvelocity2 = 500
var attackvelocity3 = 900
var airattacks = true
var lowvelocity = 20
enum States {Ground, Jumping, Falling, On_Wall, Walljumping, Walking, Crouching, Sneaking, Sliding, Attack1, Attack2, Attack3, AirAttack, Turning}
#pipe state is connected to jumping, fix that
enum WeaponStates {Fist, Pipe}
var nojumps = false
var state : int = States.Ground
var weapon : int = WeaponStates.Fist
var gravity = ProjectSettings.get_setting(\"physics/2d/default_gravity\")
var coyote = false
var coyote_frames = 3
var turnstarted = false
var lookinleft = false
var justjumped = false
var coyotestarted = false
var jumpbuffer = false
var canwallcolcheck = true
var wallslow = true
var slidecooldown = false
var timerstarted = false
var cantuncrouch = false
var sliding = false
var attacking = false 
var attack2 = false
var airattacking = false
var attackstartup = false
@onready var Standingcshape = $PlayerCol
@onready var Crouchingcshape = $CrouchCol
@onready var hitbox = $HitArea

signal paused

func _ready():
	Gamemanager.player = self
	state = WeaponStates.Fist
	state = States.Ground

func jump():
	velocity.y = lerp(velocity.y, JUMP_VELOCITY, jumpresistance) 
	justjumped = true
	if state != States.Walljumping:
		state = States.Jumping
func die():
	Gamemanager.respawn_player()
func _on_coyote_timer_timeout():
	coyote = false
func _on_jump_buffer_timeout():
	jumpbuffer = false
func _on_wall_slow_timer_timeout():
	wallslow = false
func _on_slide_timer_timeout():
		sliding = false
func _on_start_up_timer_timeout():
	attackstartup = false
func _on_wall_col_check_timer_timeout():
	canwallcolcheck = true
func _on_attack_timer_timeout():
	attacking = false
	attack2 = false
	hitbox.monitoring = false
func _on_air_attack_timer_timeout():
	airattacking = false
	hitbox.monitoring = false
func beginslide():
	if !slidecooldown and state != States.Sliding and !slidecooldown:
		if lookinleft:
			velocity.x = -slidespeed
		else:
			velocity.x = slidespeed
		state = States.Sliding
		if sliding == false:
			$SlideTimer.start()
			sliding = true
func handle_wall_jump():
	if state != States.On_Wall: return
	var wall_normal = get_wall_normal()
	#handle wall jump
	if Input.is_action_just_pressed(\"up\") and wall_normal == Vector2.LEFT and state != States.Walljumping: #and !Input.is_action_pressed(\"right\"):
		state = States.Walljumping
		walljumpbegin = true
		velocity.x = 0
		velocity.x -= walljumpvelocity
		jump()
		if lookinleft == false:
			lookinleft = true
			scale.x = -1.0
	if Input.is_action_just_pressed(\"up\") and wall_normal == Vector2.RIGHT and state != States.Walljumping: #and !Input.is_action_pressed(\"left\"):
		state = States.Walljumping
		walljumpbegin = true
		velocity.x = 0
		velocity.x += walljumpvelocity
		jump()
		if lookinleft == true:
			lookinleft = false
			scale.x = -1.0
func handle_attack():
	if Input.is_action_just_pressed(\"attack\"):
		if is_on_floor() and !Input.is_action_pressed(\"up\") and !cantuncrouch:
			if attacking == false:
				velocity.x = 0
				if lookinleft == false:
					velocity.x += attackvelocity
				else:
					velocity.x -= attackvelocity
				state = States.Attack1

		else:
			#if state == States.Jumping or state == States.Falling or state == States.Walljumping:
			if airattacks == true and !cantuncrouch:
				if airattacking == false:
					state = States.AirAttack
func handle_heavy_attack():
	if Input.is_action_just_pressed(\"heavy\"):
		print(\"tamo HEAVY\")
		pass
func _on_killzone_body_entered(_body):
	print(\"dead\")
	die()
func _physics_process(delta):
	get_wall_normal()
	var direction = Input.get_axis(\"left\", \"right\")
	if state != States.Walljumping and state != States.Sliding and state != States.Crouching:
		if direction and attacking == false:
			if is_on_floor():
				velocity.x = lerp(velocity.x, direction * SPEED, .3)
			if !is_on_floor():
				velocity.x = lerp(velocity.x, direction * SPEED, 0.08)
		else:
			if is_on_floor():
				if attacking:
					velocity.x = lerp(velocity.x, 0.0, .10)
				else:
					velocity.x = lerp(velocity.x, 0.0, .15)
			else:
				velocity.x = lerp(velocity.x, 0.0, .08)
	#flip if turned
	if direction != 0 and state != States.Walljumping and state != States.Sliding and !attacking and !airattacking:
		if direction >0 and lookinleft == true:
			lookinleft = false
			scale.x = -1.0
		if direction <0 and lookinleft == false: 
			lookinleft = true
			scale.x = -1.0
	handle_wall_jump()
	handle_attack()
	handle_heavy_attack()
	if state == States.Jumping:
		turnstarted = false
		anim.play(\"jump\")
	if state == States.Walljumping:
		if walljumpbegin == true:
			canwallcolcheck = false
		$WallColCheckTimer.start()
		anim.play(\"walljump\")
		#if velocity.x > 0:
				#Input.action_press(\"right\")
		#if velocity.x < 0:
				#Input.action_press(\"left\")
		if is_on_wall():
			if !Input.is_action_just_pressed(\"up\"):
				state = States.On_Wall

	Crouchingcshape.disabled = true
	Standingcshape.disabled = false
	if not is_on_floor():
		velocity.y += gravity * delta
		if Input.is_action_just_pressed(\"up\"):
			$JumpBuffer.start()
			jumpbuffer = true

	if $AboveCollision.is_colliding() or $AboveCollision2.is_colliding() or $AboveCollision3.is_colliding():
		cantuncrouch = true
	else:
		cantuncrouch = false

	if Input.is_action_just_pressed(\"pause\"):
		get_tree().paused = true
		emit_signal(\"paused\")
		

#Manages groundedstate
	if is_on_floor() and !cantuncrouch and state != States.Sliding and attacking != true and velocity.x >= -lowvelocity and velocity.x <= lowvelocity and state != States.Turning: #and !Input.is_action_just_pressed(\"left\") or !Input.is_action_just_pressed(\"right\"):
		state = States.Ground
	if velocity.y > 0 and !cantuncrouch and state != States.On_Wall and airattacking == false:
		state = States.Falling

#Manages floor refreshes
	if is_on_floor():
		if jumpbuffer == true:
			jump()
			jumpbuffer = false
		airattacks = true
		nojumps = false
		justjumped = false
		walljumpbegin = false
		coyotestarted = false

#handle walk state activation
	if velocity.x < -lowvelocity and is_on_floor() or velocity.x > lowvelocity and is_on_floor():
		if !Input.is_action_pressed(\"down\") and sliding == false and !cantuncrouch and (Input.is_action_pressed(\"left\") or Input.is_action_pressed(\"right\")) and attacking == false and state != States.Turning and !Input.is_action_pressed(\"attack\"):
			state = States.Walking

	#if Input.is_action_pressed(\"scan\") and state == States.Ground:
		#pass
		#enter scanning state
		#instantialize scan radar 
#Wallhang code
	if !is_on_floor() and is_on_wall():
		var wall_normal = get_wall_normal()
		if wall_normal == Vector2.RIGHT and !Input.is_action_just_pressed(\"up\") and velocity.x < 0 and ($Wallcheck.is_colliding() or $Wallcheck2.is_colliding()): #or velocity.x < 0:
			state = States.On_Wall
			velocity.y *= .85
		if wall_normal == Vector2.LEFT and !Input.is_action_just_pressed(\"up\") and velocity.x > 0 and ($Wallcheck.is_colliding() or $Wallcheck2.is_colliding()): #or velocity.x > 0:
			state = States.On_Wall
			velocity.y *= .85

	if !is_on_floor() and justjumped == false and coyotestarted == false: #and coyote == false:
		$CoyoteTimer.start()
		coyote = true
		coyotestarted = true
#Crouch code
	if state == States.Ground:
		anim.play(\"idle\")
		sliding = false
		if Input.is_action_pressed(\"down\"):
			if velocity.x >= -lowvelocity and velocity.x <= lowvelocity and !cantuncrouch:
				state = States.Crouching
#Slide code
	if state == States.Walking:
		anim.play(\"walk\")
		if Input.is_action_just_pressed(\"down\") and !slidecooldown:
			beginslide()

#More slide code
	if Input.is_action_pressed(\"down\"):
		#if wall is above character
		#disable crouch
		#SPEED = slidespeed 
		if state == States.Walking and (velocity.x > 400 or velocity.x < -400) and !slidecooldown:
			beginslide()
		if Input.is_action_just_pressed(\"right\") and !slidecooldown:
			beginslide()
		if Input.is_action_just_pressed(\"left\") and !slidecooldown:
			beginslide()
	else:
		SPEED = basespeed
	#else: 
		#Standingcshape.disabled = false
		#Crouchingcshape.disabled = true
#Sliding code...
	if state == States.Sliding:
		anim.play(\"slide\")
		Standingcshape.disabled = true
		Crouchingcshape.disabled = false
		#SPEED = slidespeed 
		if sliding == false:
			if cantuncrouch == false:
				velocity.x = lerp(velocity.x, 0.0, .2)
				slidecooldown = true
				$SlideCoolTimer.start()
		if velocity.x >= -lowvelocity and velocity.x <= lowvelocity and is_on_floor() and !cantuncrouch:
			state = States.Ground
	else:
		SPEED = basespeed
		sliding = false

	if state == States.Crouching:
		anim.play(\"crouch\")
		Standingcshape.disabled = true
		Crouchingcshape.disabled = false

#activate turn state
	#if walking left
	if velocity.x < -0 and is_on_floor() and attacking == false and state != States.Crouching  and !cantuncrouch:
		if Input.is_action_pressed(\"right\"):
			state = States.Turning
		if Input.is_action_just_pressed(\"right\"):
			state = States.Turning
	#if walking right
	if velocity.x > 0 and is_on_floor() and attacking == false and state != States.Crouching  and !cantuncrouch:
		if Input.is_action_pressed(\"left\"):
			state = States.Turning
		if Input.is_action_just_pressed(\"left\"):
			state = States.Turning

#turn state
	if state == States.Turning:
		if turnstarted == false:
			turnstarted = true
			$turnstatetimer.start()
			#will replace with turn anim
			anim.play(\"crouch\")
		#if turnstatetimer timeout
		#state = ground

	if state == States.Falling:
		if Input.is_action_just_pressed(\"attack\"):
			if airattacks == true and !cantuncrouch and airattacking == false:
					state = States.AirAttack
		anim.play(\"fall\")
		velocity.y += 20
		if is_on_floor():
			state = States.Ground


	if state == States.Attack1:
		if attacking == false:
			$StartUpTimer.start()
			$AttackTimer.start()
			attacking = true
			attackstartup = true
		hitbox.monitoring = true
		anim.play(\"attack\")
		if attackstartup == false:
			hitbox.monitoring = false
		if Input.is_action_just_pressed(\"attack\") and attacking == true and attackstartup == false and is_on_floor():
			if lookinleft == false:
				velocity.x += attackvelocity2
			else:
				velocity.x -= attackvelocity2
			state = States.Attack2
		#deactivate attack
		if attacking == false:
			hitbox.monitoring = false
			state = States.Ground

	if state == States.Attack2:
		if attack2 == false:
			$StartUpTimer.start()
			$AttackTimer.start()
			attack2 = true
			attacking = true
			attackstartup = true
		hitbox.monitoring = true
		anim.play(\"attack\")
		if attackstartup == false:
			hitbox.monitoring = false
		if Input.is_action_just_pressed(\"attack\") and attacking == true and attackstartup == false: #and attackstartup == false and is_on_floor():
			if lookinleft == false:
				velocity.x += attackvelocity3
			else:
				velocity.x -= attackvelocity3
			if state != States.Attack1:
				state = States.Attack3
		if attacking == false:
			hitbox.monitoring = false
			state = States.Ground

	if state == States.Attack3:
		if attacking == false:
			$StartUpTimer.start()
			$AttackTimer.start()
			attack2 = true
			attacking = true
			attackstartup = true
		hitbox.monitoring = true
		anim.play(\"attack\")
		if attackstartup == false and attacking == false:
			print(\"falsey\")
			hitbox.monitoring = false

	if state == States.AirAttack:
		if airattacking == false:
			airattacks = false
			airattacking = true
			velocity.y = 0
			velocity.y -= airattackvelocity
			anim.play(\"airattack\")
			$AirAttackTimer.start()
		hitbox.monitoring = true
		if airattacking == false:
			hitbox.monitoring = false
			state = States.Falling

	if state == States.On_Wall:
		airattacks = true
		if Input.is_action_just_pressed(\"right\"):
			state = States.Falling
		if Input.is_action_just_pressed(\"left\"):
			state = States.Falling
		if !is_on_wall() and !$Wallcheck.is_colliding() and !$Wallcheck2.is_colliding():
			state = States.Falling
		anim.play(\"onwall\")
		if velocity.y > 0:
			if Input.is_action_just_pressed(\"down\"):
				wallslow = false
			if wallslow == true:
				gravity = 300
				if timerstarted == false:
					$WallSlowTimer.start()
					timerstarted = true
			else:
				gravity = ProjectSettings.get_setting(\"physics/2d/default_gravity\")
				velocity.y *= 1.15
			if velocity.y > 600:
				state = States.Falling
	else:
		wallslow = true
		timerstarted = false
		gravity = ProjectSettings.get_setting(\"physics/2d/default_gravity\")

	# Handle Jump.
	if Input.is_action_just_pressed(\"up\") and (is_on_floor() or coyote) and state != States.Walljumping and !cantuncrouch:
		jump()
		
	#Handle Double Jump
	#if Input.is_action_just_pressed(\"up\") and !is_on_floor() and _state != 3 and nojumps == false and justjumped == true:
	#	jump()
	#	nojumps = true
	#if Input.is_action_just_released(\"up\"):
		#velocity.y *= .60
	if not Input.is_action_pressed(\"up\") and state == States.Jumping:
		velocity.y *= .90
	move_and_slide()


func _on_hit_area_body_entered(body):
	body.hit(damage)
	if body.knockspeed:
		if state == States.Attack1:
			body.knockback_velocity.y = -10
			body.knockspeed = 150
		if state == States.Attack2:
			body.knockback_velocity.y = -20
			body.knockspeed = 300

func _on_turnstatetimer_timeout():
	turnstarted = false
	if is_on_floor() and !attacking:
		state = States.Ground



func _on_slide_cool_timer_timeout():
	slidecooldown = false
"

